{
	"picture": {
		"prefix": "s-picture",
		"body": [
			"<picture>",
			"  <source srcset=\"@img/.webp\" type=\"image/webp\">",
			"  <img class=\"$1\" src=\"@img$2\" alt=\"$3\" width=\"$4\" height=\"$5\" loading=\"lazy\">",
			"</picture>"
		],
		"description": "Snippet for adding 'webp' and 'jpg' images in html"
	},
	"svg-sprite": {
		"prefix": "s-svg",
		"body": [
			"<svg class=\"$1\">",
			"  <use xlink:href=\"./images/sprites/icons.svg#$2\"></use>",
			"</svg>"
		],
		"description": "Snippet for use svg sprites"
	}
}